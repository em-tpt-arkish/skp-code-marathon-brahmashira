<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Brahmashira - Ancient Hindu Weapon</display-name>

	<servlet>
		<servlet-name>brahmashira</servlet-name>
		<servlet-class>me.sumithpuri.github.brahmashira.web.servlet.Brahmashira</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>brahmashira</servlet-name>
		<url-pattern>*.search</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
